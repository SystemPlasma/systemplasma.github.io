      (function enforceHost(){
        var allowed = new Set([
          'systemplasma.github.io', // production
          'localhost', '127.0.0.1', '::1' // local dev
        ]);
        if (!allowed.has(location.hostname)) {
          location.replace('https://systemplasma.github.io/wkw-deckbuilder/');
        }
      })();
    </script>
